{
  "name": "magic-code",
  "version": "0.1.0",
  "private": true,
  "main": "electron.js",
  "homepage": "./",
  "repository": "http://gitea.ohcat.xyz/utooo/magic-code.git",
  "description": "前端代码生成器",
  "dependencies": {
    "@formily/antd": "^1.0.9",
    "@formily/antd-components": "^1.0.9",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "antd": "^4.0.3",
    "electron-is-dev": "^1.1.0",
    "image-size": "^0.8.3",
    "lodash.uniqueid": "^4.0.1",
    "lowdb": "^1.0.0",
    "react": "^16.13.0",
    "react-available-hooks": "^1.0.1-beta.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "shortid": "^2.2.15",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build && node ./tasks/prepack.js",
    "test": "react-app-rewired test",
    "analyze": "yarn build && source-map-explorer 'build/static/js/*.js'",
    "dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "pack:dmg": "yarn build && electron-builder -m dmg"
  },
  "author": {
    "name": "ben",
    "email": "ben@qq.com",
    "url": "https://website.com"
  },
  "build": {
    "appId": "com.magic-code.app",
    "productName": "magic-code",
    "copyright": "Copyright © 2019 ${author}",
    "mac": {
      "icon": "app/assets/icon.png",
      "type": "production",
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg"
      ]
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.3",
    "antd-dayjs-webpack-plugin": "^0.0.8",
    "babel-plugin-import": "^1.13.0",
    "concurrently": "^5.1.0",
    "customize-cra": "^0.9.1",
    "electron": "^8.1.1",
    "electron-builder": "^22.4.1",
    "fs-extra": "^9.0.0",
    "react-app-rewired": "^2.1.5",
    "sass": "^1.26.3",
    "source-map-explorer": "^2.4.0",
    "wait-on": "^4.0.1"
  }
}
